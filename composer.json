{
    "name": "naucon/storage",
    "description": "Storage Abstraction Layer .",
    "keywords": ["key-value", "cache", "store", "storage", "abstraction", "layer"],
    "homepage": "https://github.com/naucon/storage",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Sanzenbacher"
        }
    ],
    "require": {
        "php": "^8.1",
        "naucon/utility": "~1.0",
        "naucon/registry": "~1.0",
        "doctrine/common": "^3.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "predis/predis": "^1.1",
        "doctrine/orm": "^2.0",
        "symfony/http-foundation": "^4.0 || ^5.0",
        "psr/cache": "~1.0",
        "psr/simple-cache": "~1.0"
    },
    "suggest": {
        "predis/predis": "storage adapter for redis",
        "doctrine/orm": "storage adapter for doctrine2",
        "symfony/http-foundation": "storage adapter for symfony session storage",
        "psr/cache": "storage adapter to PSR-6 Cache",
        "psr/simple-cache": "storage adapter to PSR-16 Simple Cache"
    },
    "autoload": {
        "psr-4": {"Naucon\\Storage\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Naucon\\Storage\\Tests\\": "tests"}
    }
}
